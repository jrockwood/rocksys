{
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": true,
  "files.eol": "\n",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true
  },
  "files.trimTrailingWhitespace": true,
  "prettier.bracketSpacing": true,
  "prettier.javascriptEnable": ["javascript", "javascriptreact"],
  "prettier.jsonEnable": ["json"],
  "prettier.jsxBracketSameLine": false,
  "prettier.printWidth": 120,
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "es5",
  "prettier.typescriptEnable": ["typescript", "typescriptreact"],
  "prettier.useTabs": false,
  "rewrap.doubleSentenceSpacing": false,
  "rewrap.reformat": true,
  "rewrap.wholeComment": true,
  "rewrap.wrappingColumn": 80,
  "typescript.extension.sortImports.maxNamedImportsInSingleLine": 3,
  "typescript.extension.sortImports.omitSemicolon": false,
  "typescript.extension.sortImports.pathSortOrder": ["package", "relativeUpLevel", "relativeDownLevel"],
  "typescript.extension.sortImports.quoteStyle": "single",
  "typescript.extension.sortImports.sortMethod": "path",
  "typescript.extension.sortImports.sortOnSave": false
}
